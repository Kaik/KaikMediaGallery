{% if icon is not empty and icon.original is defined %}
    {% include "KaikmediaGalleryModule:Plugin:item.html.twig" with {'settings': settings, 'original':icon.original, 'relation': icon, 'css': 'col-md-12' }  %}                 
	{% if icon.details is defined %}      
			<div class="">
			<p><strong>{{ __('Name') }}:</strong>{{ icon.details.name }}</p>
			</div>
	{% else %}
			<div class="">
			<p><strong>{{ __('Name') }}:</strong>{{ icon.original.name }}</p>
			</div>
	{% endif %}         
{% else %}
        <p class="alert alert-warning text-center"> {{ __('No icon selected') }} </p>
{% endif %}



