<div class="row" style="margin-left: -10px; margin-right:-10px;margin-bottom:0px;padding-top:7px;padding-bottom:7px;border-bottom:1px solid #d8d8d8;background-color: #f1f1f1;">
    <div class="col-md-1" >
        {{ form_widget(form.name, { 'attr': {'class': '', 'placeholder': __('Feature name') } }) }}
    </div>
    <div class="col-md-1" >
        {{ form_widget(form.title, { 'attr': {'class': '', 'placeholder': __('Display title') } }) }}
    </div>
    <div class="col-md-1">
        {{ form_widget(form.mimeTypes, { 'attr': {'class': '', 'placeholder': __('mimeTypes')} }) }}
    </div>
    <div class="col-md-1" >
        {{ form_widget(form.prefix, { 'attr': {'class': '', 'placeholder': __('File prefix') } }) }}
    </div>
    <div class="col-md-1">
        {{ form_widget(form.dir, { 'attr': {'class': '', 'placeholder': __('Sub directory')} }) }}
    </div>
    <div class="col-md-1">
        {{ form_widget(form.singleFileMaxSize, { 'attr': {'class': ''} }) }}
    </div>
    <div class="col-md-1">
        {{ form_widget(form.plugin_item_css, { 'attr': {'class': ''} }) }}
    </div>
    <div class="col-md-1">
        {{ form_widget(form.multiple, { 'attr': {'class': 'btn btn-default radio-inline btn-xs'} }) }}
    </div>
    <div class="col-md-1">
        {{ form_widget(form.extra_title, { 'attr': {'class': 'btn btn-default radio-inline btn-xs'} }) }}
    </div>
    <div class="col-md-1">
        {{ form_widget(form.extra_legal, { 'attr': {'class': 'btn btn-default radio-inline btn-xs'} }) }}
    </div>
</div>